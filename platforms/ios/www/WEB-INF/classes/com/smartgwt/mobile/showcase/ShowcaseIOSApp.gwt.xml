<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<!--
This GWT module is used to compile the Showcase application for use with PhoneGap / Apache Cordova.
-->
<module rename-to="showcase">
    <inherits name="com.smartgwt.mobile.showcase.Showcase"/>
    <set-property name="user.agent" value="ipad,iphone"/>
    <set-property name="isc.usingCordova" value="yes"/>
</module>
